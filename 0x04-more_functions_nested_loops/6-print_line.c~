#include "main.h"

#include <stdio.h>

#include <string.h>

#include <stdlib.h>

/**

 *print_line - print 0 to 9

 *@n:n is the parameter to be passed

 *

 */

void print_line(int n)
  
{
  
  int lines = n, i;
  
  if (lines >= 0)
    
    {
      
      for (i = 0; i < lines; i++)
	
	{
	  
	  _putchar('_');
	  
	}
      
    }
  
  _putchar('\n');
  
}
